<h1>Welcome to Museums</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Museum</th>
      <th>Photo</th>
    </tr>
  </thead>
  <tbody>
    <% @paintings.each do |painting| %>
      <tr>
      	<td><%= painting.title %></td>
      	<td><%= painting.artist.name %></td>
      	<td><%= painting.museum.museum_name %></td>
        <td><%= link_to painting.photo_url %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h3><%= link_to "+ Add New Painting", new_painting_path %></h3>

